package bobo;

import java.util.Random;

public class GobeletBean {
	private DeBean d1,d2;

	public GobeletBean() {

		d1 = new DeBean();
		d2 = new DeBean();

	}

	public DeBean getD1() {
		return d1;
	}

	public void setD1(DeBean d1) {
		this.d1 = d1;
	}

	public DeBean getD2() {
		return d2;
	}
	public void lancer (){
		d1.lancer();
		d2.lancer();

	}

	public void setD2(DeBean d2) {
		this.d2 = d2;

	}


public int getScore() {
	return d1.getvalue()+d2.getvalue();
}


}




