package bobo1;

import bobo.JoueurDean;

public class PartieBean2 {

	public static final int NBR_TOUR = 10;

	public static final int SCORE_A_ATTEINDRE = 7*3;

	private JoueurBean j1, j2;
	private int tourEncours;
	public PartieDebean(String nom1,String nom2) {
		j1 = new JoueurBean(nom1);
		j2 = new JoueurBean(nom2);
		tourEncours = 1;
	}
}



